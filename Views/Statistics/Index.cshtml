            <h5 style="font-family: 'Almarai', sans-serif; font-size:18px; background-color: rgb(82, 134, 203); color: azure;" class="card-header"><img class="me-3" src="~/img/report25.png" alt="">  توزيع المنشآت الصناعية بحسب النشاط وحجم الاستثمار </h5>
            <form method="post" class="card-body">
                <hr class="mx-n4 my-1">


                <input name="select_statistic_send" value="1" class="form-control" type="hidden">




                <!--قائمة حجم المنشآت منسدلة-->


                <div class="row mb-3 my-4">
                    <label style="font-family: 'Cairo', sans-serif; font-size:16px;" class="col-form-label col-md-3  text-sm-end "
                           for="alignment-country"> حجم المنشأة :</label>
                    <div class="col-sm-4">
                        <div style="font-family: 'Almarai', sans-serif; font-size:14px;" class="position-relative">

                            @if (facilities is not null)
                            {
                                List<string> total_fa_size = new List<string>();


                                <select title="اختر حجم المنشأة" data-live-search="true" name="fac_size" class="selectpicker w-100 "
                                        data-style="btn-default" tabindex="null"  required>
                                    @foreach (var item in facilities)
                                    {
                                        if (!total_fa_size.Contains(item.FaSize))
                                        {
                                            total_fa_size.Add(item.FaSize.ToString());

                                            <option value="@item.FaSize"> @item.FaSize.ToString() </option>
                                        }
                                    }
                                </select>
                            }
                            else
                            {
                                <input name="fac_size" class="form-control" type="text">

                            }
                        </div>
                    </div>
                </div>


                <!-- حقل نشاط المنشاة ----1------------------>

                <div class="row mb-3 my-4">
                    <label style="font-family: 'Cairo', sans-serif; font-size:16px;" class="col-form-label col-md-3  text-sm-end"
                           for="alignment-country"> نشاط المنشآة :</label>
                    <div style="font-family: 'Almarai', sans-serif; font-size:14px;" class="col-sm-4">


                        @if (facilities is not null)
                        {
                            List<string> total_Main_activity = new List<string>();

                            <select title="اختر نشاط المنشأة" data-live-search="true" name="search_1" class="selectpicker w-100 "
                                    data-style="btn-default" tabindex="null" required>
                                @foreach (var item in facilities)
                                {
                                    if (!total_Main_activity.Contains(item.FaMainActivity))
                                    {
                                        total_Main_activity.Add(item.FaMainActivity.ToString());

                                        <option value="@item.FaMainActivity"> @item.FaMainActivity.ToString() </option>


                                    }
                                }
                            </select>
                        }
                        else
                        {
                            <select data-live-search="true" name="search_1" class="select3 form-select select3-hidden-accessible" v
                                    data-allow-clear="true" tabindex="-1" aria-hidden="true" title="لاتوجد بيانات">
                            </select>
                        }

                    </div>
                </div>






                <!-- حقل حجم الاستثمار ------2---------------->

                <div class="row mb-3 my-4">
                    <label style="font-family: 'Cairo', sans-serif; font-size:16px;" class="col-form-label col-md-3  text-sm-end "
                           for="alignment-country"> حجم الاستثمار :</label>
                    <div style="font-family: 'Almarai', sans-serif; font-size:14px;" class="col-sm-4">
                        @if (facilities is not null)
                        {
                            List<string> total = new List<string>();

                            <select title="اختر حجم اللأستثمار" data-live-search="true" name="search_2" class="selectpicker w-100 "
                                    data-style="btn-default" tabindex="null" required>
                                @foreach (var item in facilities)
                                {
                                    if (!total.Contains(item.FaShareCapital))
                                    {
                                        total.Add(item.FaShareCapital.ToString());

                                        <option value="@item.FaShareCapital"> @item.FaShareCapital.ToString() </option>
                                    }
                                }
                            </select>
                        }
                        else
                        {
                            <select data-live-search="true" name="search_2" class="select3 form-select select3-hidden-accessible" v
                                    data-allow-clear="true" tabindex="-1" aria-hidden="true" title="لاتوجد بيانات">
                            </select>
                        }
                    </div>
                </div>




                <!-- ازرار الاستعلام والتهيئة ---------------------->

                <div class="pt-4">
                    <div class="row justify-content-end">
                        <div style="font-family: 'Almarai', sans-serif; font-size:16px;" class="col-sm-7">
                            <button type="submit" id="statistic_find" onclick="handleForm()" class="btn btn-primary me-1 me-sm-2"> استعلام</button>
                            <button type="reset" class="btn btn-label-secondary">تهيئة</button>
                        </div>
                    </div>
                </div>




            </form>

        </div>
    </div>


